namespace CanaryLib;

enum Client_t : uint8 { 
  UNKNOWN = 0x0,
  CANARY = 0x13
}

table Challenge {
  timestamp:uint32;
  random:uint8;
}

table GameLoginInfo {
  session_key:string;
  char_name:string;
  challenge:Challenge;
}

table LoginInfo {
  account:string;
  auth_token:string;
  password:string;
  xtea_key:[uint32];
  game_login_info:GameLoginInfo;
  client:Client_t = UNKNOWN;
}
root_type LoginInfo;